{
  "name": "mcpui-travel-planner",
  "version": "1.0.0",
  "description": "MCP-UI Travel Planner - Learning Project",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "install:all": "npm install && npm install --workspace=server && npm install --workspace=client",
    "build:server": "npm run build --workspace=server",
    "build:client": "npm run build --workspace=client",
    "build": "npm run build:server && npm run build:client",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "dev": "npm run build:server && concurrently \"npm run bridge --workspace=server\" \"npm run dev:client\"",
    "bridge": "npm run bridge --workspace=server",
    "start:server": "npm run start --workspace=server"
  },
  "keywords": [
    "mcp",
    "mcp-ui",
    "travel-planner"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
